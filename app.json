{
  "expo": {
    "name": "EchoShield",
    "slug": "EchoShield",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/EcoShield-mainLogo.png",
    "scheme": "echo-shield",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/EcoShield-SplashLogo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "UIBackgroundModes": ["fetch", "remote-notification"],
        "NSUserNotificationUsageDescription": "This app needs access to notifications to alert you of important updates."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/EcoShield-mainLogo.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": ["NOTIFICATIONS"]
    },
    "web": {
      "bundler": "metro",
      "output": "server",
      "favicon": "./assets/images/EcoShield-mainLogo.png"
    },
    "plugins": [
   [  
    
   "expo-router",
      {
        "origin" : "https://ecoshield.com/"
      }
    
    ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
